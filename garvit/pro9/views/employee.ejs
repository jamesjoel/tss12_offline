<%- include('header') %>
<div class="container mt-3" style="min-height: 600px;">
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <table class="table table-dark table-bordered table-hover table-stiped">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Age</th>
                    <th>City</th>
                    <th>Salary</th>
                </tr>
                <% info.forEach(function(x, n){ %>
                    <tr>
                        <td><%= n+1 %></td>
                        <td><%= x.name %></td>
                        <td><%= x.age %></td>
                        <td><%= x.city %></td>
                        <td><%= x.salary %></td>
                    </tr>

                <% }) %>



            </table>
        </div>
    </div>
</div>
<%- include('footer') %>